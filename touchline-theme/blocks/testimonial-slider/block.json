{
  "apiVersion": 2,
  "name": "touchline/testimonial-slider",
  "title": "Testimonial Slider",
  "category": "widgets",
  "description": "Accessible testimonial slider.",
  "textdomain": "touchline",
  "attributes": {
    "autoplay": {
      "type": "boolean",
      "default": false
    },
    "showDots": {
      "type": "boolean",
      "default": true
    },
    "interval": {
      "type": "number",
      "default": 6
    }
  },
  "supports": {
    "html": false,
    "align": ["wide", "full"],
    "anchor": true
  },
  "editorScript": "file:./editor.js",
  "style": "file:./style.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}
